            >
            ["id5.1"] = <
                text = <"収縮期">
                description = <"1つ以上の脈の間で最高値を示す全身の動脈圧 - 心機図の収縮期で測定される">
            >
            ["id34.1"] = <
                text = <"コメント">
                description = <"血圧測定のコメント">
            >
        >
        ["de"] = <
